		{
			"name": "Filter: Get all active todos",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let jsonData = pm.response.json()\r",
							"\r",
							"pm.test('Verify if there is any completed todo', () => {\r",
							"    _.each(jsonData, (item) => {\r",
							"        pm.expect(item.completed).to.not.equal(true);\r",
							"    })\r",
							"})\r",
							"\r",
							"pm.test(\"Verify status of get:/todos?completed=false is 200\", function () {\r",
							"        pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Response time is less than 2000 ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
							"});"
						],
