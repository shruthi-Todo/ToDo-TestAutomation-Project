		{
			"name": "Update a todo",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verify if it returns title as requested to be updated\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    var req = JSON.parse(pm.request.body);\r",
							"    pm.expect(jsonData.title).to.equal(req.title);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify if it returns completed as requested to be updated\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    var req = JSON.parse(pm.request.body);\r",
							"    pm.expect(jsonData.completed).to.equal(req.completed);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify if it returns userId as requested to be updated\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    var req = JSON.parse(pm.request.body);\r",
							"    pm.expect(jsonData.userId).to.equal(req.userId);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify if it returns id against which update is performed\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    let val = parseInt(pm.variables.get('id'));\r",
							"    pm.expect(jsonData.id).to.equal(val);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify status of put:/todos/25 is 200\", function () {\r",
							"        pm.response.to.have.status(200);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
