				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Verify status of post:/todos is 201\", function () {\r",
							"        pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify if it returns completed as false\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    var req = JSON.parse(pm.request.body);\r",
							"    pm.expect(jsonData.completed).to.equal(req.completed);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify if it returns userId as passed in request\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    var req = JSON.parse(pm.request.body);\r",
							"    pm.expect(jsonData.userId).to.equal(req.userId);\r",
							"});\r",
							"\r",
							"pm.test(\"Verify if it returns title as passed in request\", function () {\r",
							"    var jsonData = pm.response.json();   \r",
							"    var req = JSON.parse(pm.request.body);\r",
							"    pm.expect(jsonData.title).to.equal(req.title);\r",
